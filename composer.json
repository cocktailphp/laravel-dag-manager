{
    "name": "telkins/laravel-dag-manager",
    "description": "A SQL-based Directed Acyclic Graph (DAG) solution for Laravel.",
    "keywords": [
        "telkins",
        "laravel-dag-manager",
        "sql",
        "dag",
        "directed",
        "acyclic",
        "graph",
        "laravel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Elkins",
            "email": "travis.elkins+github@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.1.3",
        "illuminate/database": "~5.6.0|~5.7.0|~5.8.0",
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.6.0|~3.7.0|~3.8.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Telkins\\Dag\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Telkins\\Dag\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Telkins\\Dag\\Providers\\DagServiceProvider"
            ]
        }
    }
}
